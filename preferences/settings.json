{
  "workbench.colorTheme": "Activate UMBRA protocol",
  "workbench.iconTheme": "material-icon-theme",
  "vscode_custom_css.imports": [
      "path_to_file:Synthwave84.css"
  ],
  "editor.fontFamily": "Fira Code, Menlo, pixelmix, Pixelade, Cascadia Code, Hack, Anonymous Pro, Monaco, 'Courier New', monospace",
  "editor.fontWeight": "300", // Light
  "editor.fontLigatures": true,
  "workbench.panel.defaultLocation": "right",
  "terminal.integrated.rightClickBehavior": "default",
  "terminal.integrated.fontFamily": "Fira Code",
  "editor.accessibilityPageSize": 8,
  "editor.fontSize": 12,
  "editor.letterSpacing": 0.5,
  "editor.lineHeight": 16,
  "editor.cursorBlinking": "expand",
  "editor.cursorSmoothCaretAnimation": true,
  "editor.gotoLocation.multipleDefinitions": "goto",
  "editor.snippetSuggestions": "inline",
  "editor.quickSuggestions": {
      "comments": true
  },
  "problems.showCurrentInStatus": true,
  "editor.suggestSelection": "first",
  "editor.renderLineHighlight": "all",
  "editor.minimap.enabled": true,
  "editor.renderWhitespace": "none",
  "editor.renderControlCharacters": false,
  "workbench.sideBar.location": "left",
  "workbench.activityBar.visible": true,
  "todo-tree.tree.showScanModeButton": false,
  "search.collapseResults": "alwaysExpand",
  "files.eol": "\n",
  "files.insertFinalNewline": true,
  "files.trimTrailingWhitespace": true,
  "editor.linkedEditing": true,
  "workbench.editor.wrapTabs": true,
  "window.dialogStyle": "custom",
  "terminal.integrated.tabs.enabled": true,
  "editor.autoClosingDelete": "always",
  "update.enableWindowsBackgroundUpdates": false,
  "editor.autoClosingBrackets": "always",
  "editor.bracketPairColorization.enabled": true,
  "workbench.editor.tabSizing": "shrink",
  "editor.suggest.preview": true,
  "editor.tabSize": 2,
  "editor.cursorStyle": "line",
  "window.zoomLevel": 1
}
